{
    "ace": {
        "@guessTabSize": "true",
        "@newLineMode": "unix",
        "@tabSize": "4",
        "@useSoftTabs": "true"
    },
    "build": {
        "@builder": "auto",
        "@path": "/.c9/builders",
        "@saveall": true
    },
    "find.nak": {
        "@searchLimit": 100
    },
    "language": {
        "@eslintrc": "true",
        "@instanceHighlight": "true",
        "@semi": "true",
        "@undeclaredVars": "true",
        "@unusedFunctionArgs": "false",
        "@warnLevel": "info"
    },
    "run": {
        "@path": "/.c9/runners",
        "configs": {
            "@inited": "true",
            "json()": {
                "express": {
                    "command": "bin/www",
                    "cwd": "/",
                    "debug": true,
                    "env": {
                        "DEBUG": "main"
                    },
                    "name": "express",
                    "runner": "Shell command"
                },
                "mongo": {
                    "command": "mongod --nojournal --dbpath=./data",
                    "cwd": "/",
                    "debug": false,
                    "name": "mongo",
                    "runner": "Shell command"
                },
                "start express": {
                    "command": "bin/www",
                    "cwd": "/serverside",
                    "debug": false,
                    "env": {},
                    "name": "start express",
                    "runner": "Node.js (default)"
                },
                "www": {
                    "command": "bin/www",
                    "cwd": "/",
                    "debug": true,
                    "env": {
                        "DEBUG": "main"
                    },
                    "name": "www",
                    "runner": "Node.js (default)"
                }
            }
        }
    },
    "share": {
        "@app": false,
        "@preview": false,
        "@useOwnerSettings": false
    }
}